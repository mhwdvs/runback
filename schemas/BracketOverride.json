{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "additionalProperties": false,
    "properties": {
        "should_override": {
            "type": "boolean",
            "default": false
        },
        "override": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "bracket_side": {
                    "type": "number"
                },
                "bracket_progress": {
                    "type": "number"
                },
                "finals_progress": {
                    "type": "number"
                }
            }
        }
    },
    "required": [
        "override",
        "should_override"
    ]
}
